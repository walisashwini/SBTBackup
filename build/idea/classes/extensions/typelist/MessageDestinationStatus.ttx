<?xml version="1.0"?>
<typelistextension xmlns="http://guidewire.com/typelists" name="MessageDestinationStatus" desc="Message Destination Status">
  <typefilter
    desc="Defines list of MessageDestinationStatus for DocumentStore which indicates its unavailability"
    name="Unavailable">
    <include
      code="Shutdown"/>
    <include
      code="Suspended"/>
    <include
      code="Suspending"/>
    <include
      code="SuspendedInbound"/>
    <include
      code="SuspendedOutbound"/>
    <include
      code="Unknown"/>
  </typefilter>
</typelistextension>

<?xml version="1.0"?>
<typelistextension xmlns="http://guidewire.com/typelists" name="SpecialHandling" desc="Special handling for a Policy Change.">
  <typefilter
    desc="Special handling types to show accurate billing special handling instructions for policies with no final audit."
    name="SpecialHandlingForNonAudit">
    <include
      code="billimmediately"/>
    <include
      code="billonnext"/>
  </typefilter>
  <typefilter
    desc="Special handling types to show accurate billing special handling instructions for policies with open/scheduled final audit."
    name="SpecialHandlingForAudit">
    <include
      code="holdforaudit"/>
  </typefilter>
</typelistextension>
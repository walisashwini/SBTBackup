<div class="gw-ListView--UI-section">
  <div class="gw-ListView--UI-left">
    <div :if="title" role="heading" aria-level="2" class="gw-ListView--UI--title">${title}</div>
    ${lvToolbar}
    <div :if="lvFilters" class="gw-ListView--UI-filters" role="toolbar" aria-label="${ariaFilterToolbarLabel}">
      ${lvFilters}
    </div>
  </div>
  <div class="gw-ListView--UI-right">
    ${:children}
  </div>
</div>

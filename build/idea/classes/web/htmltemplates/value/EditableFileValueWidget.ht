<input type="file"
       id="${renderId}--file"
       style="display: none" disabled="${disabled}" multiple="${multiple}"
       data-gw-postOnChange="${onChange}"/>
<input :if="valueDisplayed" class="gw-displayed-value" type="text" id="${renderId}--text" readonly value="${currentValue}"/>
<div id="${renderId}--browseButton" data-gw-click="gwFileValue.browseHandler id:${renderId}" class="gw-FileInputWidget--button ${valueDisplayed ? 'gw-value-displayed' : ''}">${buttonText}</div>

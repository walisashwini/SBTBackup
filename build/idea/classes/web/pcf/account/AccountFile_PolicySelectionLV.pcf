<?xml version="1.0"?>
<PCF
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="../../../../../pcf.xsd">
  <ListViewPanel
    id="AccountFile_PolicySelectionLV">
    <ExposeIterator
      valueType="entity.PolicyPeriod"
      widget="AccountFile_PolicySelectionLV"/>
    <Require
      name="visiblePolicies"
      type="List&lt;PolicyPeriod&gt;"/>
    <RowIterator
      canPick="false"
      editable="true"
      elementName="aPeriod"
      hasCheckBoxes="true"
      value="visiblePolicies"
      valueType="java.util.List&lt;entity.PolicyPeriod&gt;">
      <Row
        useArchivedStyle="aPeriod.PolicyTerm.CheckArchived">
        <TextCell
          action="gw.web.account.AccountFileUIHelper.retrievePolicy(aPeriod)"
          id="PolicyNumber"
          label="DisplayKey.get(&quot;Web.AccountFile.Summary.PolicyTermsLV.PolicyNumber&quot;)"
          sortOrder="4"
          value="aPeriod.PolicyNumberDisplayString"/>
        <TextCell
          enableSort="false"
          id="Product"
          label="DisplayKey.get(&quot;Web.AccountFile.Summary.PolicyTermsLV.Product&quot;)"
          sortOrder="1"
          value="aPeriod.Policy.Product"
          valueType="gw.api.productmodel.Product"/>
        <TextCell
          enableSort="false"
          id="Status"
          label="DisplayKey.get(&quot;Web.AccountFile.Summary.PolicyTermsLV.Status&quot;)"
          value="aPeriod.PeriodDisplayStatus"/>
        <DateCell
          id="EffectiveDate"
          label="DisplayKey.get(&quot;Web.AccountFile.Summary.PolicyTermsLV.EffectiveDate&quot;)"
          sortOrder="2"
          value="aPeriod.PeriodStart"/>
        <DateCell
          id="ExpirationDate"
          label="DisplayKey.get(&quot;Web.AccountFile.Summary.PolicyTermsLV.ExpirationDate&quot;)"
          sortOrder="3"
          value="aPeriod.PeriodEnd"/>
      </Row>
    </RowIterator>
  </ListViewPanel>
</PCF>
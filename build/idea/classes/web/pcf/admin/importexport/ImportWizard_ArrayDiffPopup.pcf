<?xml version="1.0"?>
<PCF
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="../../../../../../pcf.xsd">
  <Popup
    canVisit="perm.User.importadmindata"
    id="ImportWizard_ArrayDiffPopup"
    parent="ImportWizard()"
    title="DisplayKey.get(&quot;Web.Admin.ImportWizard.ArrayDiffPopup.Title&quot;)">
    <LocationEntryPoint
      signature="ImportWizard_ArrayDiffPopup(arrayDifferences : gw.api.admin.ImportDataArrayDifferences)"/>
    <Variable
      name="arrayDifferences"
      type="gw.api.admin.ImportDataArrayDifferences"/>
    <Screen
      id="ImportWizard_ArrayDiffPopupScreen">
      <Toolbar/>
      <DetailViewPanel>
        <InputColumn>
          <Label
            label="DisplayKey.get(&quot;Web.Admin.ImportWizard.ArrayDiffPopup.NoRemovedElements&quot;)"
            visible="!arrayDifferences.hasRemovedElements()"/>
          <Label
            label="DisplayKey.get(&quot;Web.Admin.ImportWizard.ArrayDiffPopup.RemovedElements&quot;)"
            visible="arrayDifferences.hasRemovedElements()"/>
          <ListViewInput
            labelAbove="true"
            visible="arrayDifferences.hasRemovedElements()">
            <Toolbar/>
            <ListViewPanel
              id="ImportWizard_RemovedArrayElementsLV">
              <!-- first, the header row -->
              <Row
                renderAsSmartHeader="true">
                <CellIterator
                  elementName="field"
                  id="headerCells"
                  value="arrayDifferences.OrderedFieldNames"
                  valueType="java.lang.String[]">
                  <TextCell
                    id="field"
                    value="field"/>
                </CellIterator>
              </Row>
              <!-- Then, all the other rows -->
              <RowIterator
                editable="false"
                elementName="element"
                pageSize="0"
                value="arrayDifferences.RemovedElements"
                valueType="gw.pl.persistence.core.Bean[]">
                <Row>
                  <CellIterator
                    elementName="field"
                    value="arrayDifferences.OrderedFieldNames"
                    valueType="java.lang.String[]">
                    <TextCell
                      id="ValueCell"
                      value="element.getFieldValue(field)"
                      valueType="java.lang.Object"/>
                  </CellIterator>
                </Row>
              </RowIterator>
            </ListViewPanel>
          </ListViewInput>
          <Label
            label="DisplayKey.get(&quot;Web.Admin.ImportWizard.ArrayDiffPopup.NoAddedElements&quot;)"
            visible="!arrayDifferences.hasAddedElements()"/>
          <Label
            label="DisplayKey.get(&quot;Web.Admin.ImportWizard.ArrayDiffPopup.AddedElements&quot;)"
            visible="arrayDifferences.hasAddedElements()"/>
          <ListViewInput
            labelAbove="true"
            visible="arrayDifferences.hasAddedElements()">
            <Toolbar/>
            <ListViewPanel
              id="ImportWizard_AddedArrayElementsLV">
              <!-- first, the header row -->
              <Row
                renderAsSmartHeader="true">
                <CellIterator
                  elementName="field"
                  id="headerCells"
                  value="arrayDifferences.OrderedFieldNames"
                  valueType="java.lang.String[]">
                  <TextCell
                    id="header"
                    value="field"/>
                </CellIterator>
              </Row>
              <!-- Then, all the other rows -->
              <RowIterator
                editable="false"
                elementName="element"
                pageSize="0"
                value="arrayDifferences.AddedElements"
                valueType="gw.pl.persistence.core.Bean[]">
                <Row>
                  <CellIterator
                    elementName="field"
                    value="arrayDifferences.OrderedFieldNames"
                    valueType="java.lang.String[]">
                    <TextCell
                      id="ValueCell"
                      value="element.getFieldValue(field)"
                      valueType="java.lang.Object"/>
                  </CellIterator>
                </Row>
              </RowIterator>
            </ListViewPanel>
          </ListViewInput>
        </InputColumn>
      </DetailViewPanel>
    </Screen>
  </Popup>
</PCF>
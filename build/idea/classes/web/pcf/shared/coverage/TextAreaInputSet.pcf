<?xml version="1.0"?>
<PCF
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="pcf.xsd">
  <InputSet
    id="TextAreaInputSet">
    <Require
      name="displayable"
      type="gw.lob.common.displayable.Displayable&lt;String&gt;"/>
    <Require
      name="scheduledItem"
      type="ScheduledItem"/>
    <Require
      name="schedulePropertyInfo"
      type="gw.api.productmodel.SchedulePropertyInfo"/>
    <Require
      name="fieldDependencyInput"
      type="boolean"/>
    <Require
      name="wizard"
      type="gw.api.web.job.JobWizardHelper"/>
    <TextAreaInput
      confirmMessage="displayable.ConfirmMessage"
      editable="displayable.Editable"
      id="Displayable"
      label="displayable.Label"
      numRows="4"
      required="displayable.Required"
      showConfirmMessage="displayable.ShowConfirmMessage"
      value="displayable.Value"
      visible="not fieldDependencyInput and displayable.Visible">
      <PostOnChange
        onChange="displayable.onChange(wizard)"/>
    </TextAreaInput>
    <TextAreaInput
      confirmMessage="displayable.ConfirmMessage"
      editable="displayable.Editable"
      id="FieldDependencyDisplayable"
      label="displayable.Label"
      numRows="4"
      required="displayable.Required"
      showConfirmMessage="displayable.ShowConfirmMessage"
      value="displayable.Value"
      visible="fieldDependencyInput and displayable.Visible">
      <PostOnChange
        onChange="displayable.onChange(wizard)"/>
    </TextAreaInput>
  </InputSet>
</PCF>
<?xml version="1.0"?>
<PCF
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="../../../../../pcf.xsd">
  <TabBar
    id="ErrorTabBar">
    <Tab
      action="ErrorGroup.go()"
      id="ErrorTab"
      label="DisplayKey.get(&quot;Web.Error&quot;)"/>
    <TabBarLink
      action="Help.push()"
      id="HelpTabBarLink"
      label="DisplayKey.get(&quot;Web.TabBar.Help&quot;)"
      visible="false"/>
    <TabBarLink
      action="javascript:gw.globals.gwApp.popupAboutWindow();"
      id="AboutTabBarLink"
      label="DisplayKey.get(&quot;Web.TabBar.About&quot;)"/>
    <TabBarLink
      action="gw.api.util.NavUtil.goToDefaultEntryPoint()"
      id="ReturnTabBarLink"
      label="DisplayKey.get(&quot;Web.TabBar.ReturnToApp&quot;)"/>
    <TabBarLogout
      action="logout:*"
      id="LogoutTabBarLink"
      label="DisplayKey.get(&quot;Web.TabBar.Logout&quot;, entity.User.util.CurrentUser)"
      shortcut="Z"/>
  </TabBar>
</PCF>
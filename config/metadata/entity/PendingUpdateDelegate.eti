<?xml version="1.0"?>
<delegate
  xmlns="http://guidewire.com/datamodel"
  extendable="true"
  name="PendingUpdateDelegate">
  <column
    desc="The time when this pending update should be applied to the target entity."
    name="PendingUpdateTime"
    nullok="true"
    type="datetime"/>
  <foreignkey
    fkentity="Job"
    name="Job"
    nullok="true"/>
  <fulldescription><![CDATA[A entity storing a pending change to an account level entity.]]></fulldescription>
</delegate>
<div :if="icon" class="gw-TitleBar--icon">${icon}</div>
<div class="gw-TitleBar--titles--container">
  <div class="gw-TitleBar--title ${action ? 'gw-title-is-link'}" role="heading" aria-level="1"
       data-gw-click="?{action} fireEvent id:${renderId}">${title}</div>
  <div :if="subtitle" class="gw-TitleBar--subtitle" role="doc-subtitle">${subtitle}</div>
</div>
<div class="gw-TitleBar--children" role="toolbar">
  ${:children}
</div>

<div :if="horizontalChart" class="gw-chart-legend gw-chart-legend-horizontal ${hasDualLegend ? 'gw-chart-legend-dual'}" role="status">
  <div :if="hasDualLegend" class="gw-chart-legend gw-chart-legend-left"></div>
  <div :if="hasDualLegend" class="gw-chart-legend gw-chart-legend-right"></div>
</div>

<div class="gw-tcp-ChartContainer">
  <svg class="gw-tcp-chart-svg" role="img"></svg>
</div>
<div class="gw-tcp-Spacer" aria-hidden="true"></div>
<div :if="pieChart" class="gw-chart-legend gw-chart-legend-vertical" role="status"></div>
<input type="hidden" data-gw-chart-id="${renderId}" data-gw-chart-spec="${chartJson}" data-gw-chart-should-render>

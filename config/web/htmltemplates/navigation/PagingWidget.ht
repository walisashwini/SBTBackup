<div id="p--prev--${focusIdRoot}" class="gw-Paging--prev" aria-disabled="${!prevPage ? 'true' : 'false'}" data-gw-click="?{prevPage} gwPaging.page id:^ page:${prevPage}"><div class="gw-icon"></div></div>
<div id="p--next--${focusIdRoot}" class="gw-Paging--next" aria-disabled="${!nextPage ? 'true' : 'false'}" data-gw-click="?{nextPage} gwPaging.page id:^ page:${nextPage}"><div class="gw-icon"></div></div>
<div :if="isHierarchical" id="p--parent--${focusIdRoot}" class="gw-Paging--parent" aria-disabled="${!parentPage ? 'true' : 'false'}" data-gw-click="?{parentPage} gwPaging.parent id:^"><div class="gw-icon"></div></div>
<div :if="isHierarchical" id="p--child--${focusIdRoot}" class="gw-Paging--child" aria-disabled="${!childPage ? 'true' : 'false'}" data-gw-click="?{childPage} gwPaging.child id:^"><div class="gw-icon"></div></div>

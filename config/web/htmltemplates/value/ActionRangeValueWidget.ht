<div :for="option" :in="selectedOptions" class="gw-RangeValue ${option.retired ? 'gw-retired'}" data-gw-value="${option.value}">
  <img :if="option.icon" src="${option.icon}" alt="${option.label || ''}"/>
  <div :if="option.label" class="gw-link gw-label" data-gw-click="fireEvent id:${renderId}">${option.label}</div>
</div>

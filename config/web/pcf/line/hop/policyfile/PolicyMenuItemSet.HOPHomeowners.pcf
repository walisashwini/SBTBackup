      name="asOfDate"
      type="java.util.Date"/>
    <MenuItem
      action="PolicyFile_PolicyInfo.go(period, asOfDate)"
      id="PolicyMenuItemSet_PolicyInfo"
      label="DisplayKey.get(&quot;Web.PolicyFile.PolicyInfo&quot;)"/>
    <MenuItem
      action="PolicyFile_HOPDwelling.go(period, asOfDate)"
      id="HOPDwelling"
      label="DisplayKey.get(&quot;Web.PolicyFile.HOP.PolicyFile_HOPDwelling&quot;)"/>
    <MenuItem
      action="PolicyFile_HOPDwellingConstruction.go(period, asOfDate)"
      id="HOPDwellingConstruction"
      label="DisplayKey.get(&quot;Web.PolicyFile.HOP.PolicyFile_HOPDwellingConstruction&quot;)"/>
    <MenuItem
      action="PolicyFile_HOPCoverages.go(period, asOfDate)"
      id="HOPCoverages"
      label="DisplayKey.get(&quot;Web.PolicyFile.HOP.PolicyFile_HOPCoverages&quot;)"/>
    <MenuItem
      action="PolicyFile_HOPModifiers.go(period, asOfDate)"
      id="HOPModifiers"
      label="DisplayKey.get(&quot;Web.PolicyFile.HOP.PolicyFile_HOPModifiers&quot;)"/>
    <MenuItem
      action="PolicyFile_Pricing.go(period, asOfDate)"
      id="PolicyMenuItemSet_Pricing"
      label="DisplayKey.get(&quot;Web.PolicyFile.Pricing&quot;)"/>
    <MenuItem
      action="PolicyFile_Forms_HOP.go(period, asOfDate)"
      id="PolicyMenuItemSet_Endorsements"
      label="DisplayKey.get(&quot;Web.PolicyFile.Endorsements&quot;)"/>
  </MenuItemSet>
</PCF>

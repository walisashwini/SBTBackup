<?xml version="1.0"?>
<PCF
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="../../../../../pcf.xsd">
  <LoginPage
    afterEnter="loginForm.checkDirectHTTPLogin()"
    autosaveable="false"
    canEdit="true"
    countsAsWork="false"
    id="Login"
    startInEditMode="true"
    title="&quot;&quot;">
    <LocationEntryPoint
      signature="Login(target : pcf.api.Destination, entryException : java.lang.Exception)"/>
    <Variable
      name="target"
      type="pcf.api.Destination"/>
    <Variable
      name="entryException"
      type="java.lang.Exception"/>
    <Variable
      initialValue="new gw.api.web.util.PCLoginForm(target, entryException)"
      name="loginForm"
      type="gw.api.util.LoginForm"/>
    <Screen
      id="LoginScreen">
      <PanelColumn>
        <Spacer
          classList="app--login-page--stripes"/>
        <Spacer
          classList="app--login-page--logo"/>
        <Spacer
          classList="app--login-page--name"/>
        <PanelRef
          def="LoginDV(loginForm)">
          <Verbatim
            id="LoginFormMessage"
            label="loginForm.Message"/>
        </PanelRef>
      </PanelColumn>
    </Screen>
  </LoginPage>
</PCF>